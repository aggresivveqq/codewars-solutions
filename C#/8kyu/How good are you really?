public class Kata
{
    public static bool BetterThanAverage(int[] ClassPoints, int YourPoints)
    {
        int maxValue = 0;
        int personRate = ClassPoints.Length;
      for(int i =0;i<personRate;i++){
         maxValue += ClassPoints[i];
      }
      double avg = maxValue / personRate;
     return avg < YourPoints;
    }
}
